package aula05;

public class Exercicio02 {

	public static void main(String[] args) {
		int numero = 4;
		String resultado;
		
		switch (numero) {
		case 1:
			resultado = "Um";
			break;
			
		case 2:
			resultado = "Dois";
			break;
			
		case 3:
			resultado = "Tres";
			break;
			
			default:
				resultado = "Outro numero";
				break;
		}
		
		resultado = (numero > 0 && numero <+ 3) ? resultado.toUpperCase() : resultado;
		
		System.out.println("Resultado:" + resultado);

	}

}
